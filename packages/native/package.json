{
  "name": "@react-native-iconify/native",
  "version": "0.0.0",
  "description": "React Native Iconify component for rendering Iconify icons across platforms.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.tsx --dts --format esm,cjs --out-dir dist",
    "clean": "rm -rf dist",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "test": "echo \"No tests yet\""
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-native": ">=0.76.0",
    "react-native-svg": "^15.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.46"
  }
}

